<h1>Norrington Rank for <%= @year %></h1>

<table class='table'>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Score</th>
    </tr>
  </thead>
  <tbody>
    <% @data.each do |name, type, score| %>
      <tr>
        <td><%= name %></td>
        <td><%= type %></td>
        <td><%= score %></td>
      </tr>
    <% end %>
  </tbody>
</table>

